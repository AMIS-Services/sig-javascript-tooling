{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-plugin-prettier",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": [
            true,
            {
                "trailingComma": "es5",
                "tabWidth": 4,
                "semi": false,
                "singleQuote": false
            }
        ],
        "max-line-length": [
            true,
            {
                "limit": 200
            }
        ],
        "radix": [
            false
        ],
        "arrow-parens": [
            false
        ],
        "curly": [
            false
        ],
        "ordered-imports": false,
        "member-access": false,
        "object-literal-sort-keys": [
            false
        ],
        "member-ordering": [
            false
        ],
        "variable-name": [
            false
        ],
        "max-classes-per-file": false,
        "no-consecutive-blank-lines": [
            true,
            2
        ]
    },
    "rulesDirectory": []
}